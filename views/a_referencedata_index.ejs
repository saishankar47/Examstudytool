                                                            <script>
                                                                const collapseButtons = document.querySelectorAll(
                                                                    '[data-bs-toggle="collapse"]'
                                                                );
    
    
                                                                collapseButtons.forEach((button) => {
                                                                    button.addEventListener("click", () => {
                                                                        // Get the target collapse element
                                                                        const targetCollapse = document.querySelector(
                                                                            button.getAttribute("data-bs-target")
                                                                        );
                                                                        // Close all other collapses except the current one
                                                                        const otherCollapses = document.querySelectorAll(
                                                                            ".collapse.show:not(" +
                                                                            button.getAttribute("data-bs-target") +
                                                                            ")"
                                                                        );
    
                                                                        otherCollapses.forEach((collapse) => {
                                                                            const collapseButton = document.querySelector(
                                                                                '[data-bs-target="#' +
                                                                                collapse.getAttribute("id") +
                                                                                '"]'
                                                                            );
                                                                            collapseButton.setAttribute('aria-expanded', false);
                                                                            collapseButton.classList.remove("active");
                                                                            collapseButton.classList.add("collapsed");
                                                                            collapse.classList.remove("show");
                                                                        });
    
                                                                        // Toggle the state of the target collapse
                                                                        // console.log(targetCollapse.classList);
                                                                        if (targetCollapse.classList.contains("show")) {
                                                                            button.setAttribute("aria-expanded", false);
                                                                            button.classList.remove("active");
                                                                            button.classList.add("collapsed");
                                                                            targetCollapse.classList.remove("show");
                                                                        } else {
                                                                            button.setAttribute("aria-expanded", true);
                                                                            button.classList.add("active");
                                                                            button.classList.remove("collapsed");
                                                                            targetCollapse.classList.add("show");
                                                                        }
                                                                    });
                                                                });
                                                            </script>
    
